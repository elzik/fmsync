{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "C:\\ProgramData\\fmsync\\Elzik.FmSync.Console.log" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "FrontMatterOptions": {
    "TimeZoneId": ""
  },
  "FileSystemOptions": {
    "FilenamePattern": "*.md"
  }
}